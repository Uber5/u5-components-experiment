{
  "name": "@u5/components-experiment",
  "scripts": {
    "start": "node dist",
    "build": "babel src/ -d dist/ --source-maps --ignore \"**/__tests__/*.js\",**/*.test.js",
    "prepublish": "yarn lint && yarn typecheck && yarn test && yarn nsp:check && yarn build && yarn style-publish",
    "nsp:check": "nsp check --reporter minimal",
    "test": "jest",
    "lint": "eslint src",
    "typecheck": "flow --quiet",
    "style": "node-sass-chokidar src/styles -o styles",
    "style-publish": "yarn style && cp styles/* dist/",
    "dev:watch": "watch 'npm-run-all typecheck test lint style' src",
    "docs": "documentation readme src --section=API",
    "clean": "rimraf dist",
    "prebuild": "yarn run docs && yarn run clean",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "version": "0.0.1",
  "description": "Experimenting with yeoman generators",
  "main": "dist",
  "dependencies": {
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "recompose": "^0.26.0",
    "react-transition-group": "^1.2.1"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "documentation": "6.1.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jest": "21.14.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.3",
    "fb-watchman": "2.0.0",
    "flow-bin": "0.67.1",
    "jest": "22.4.2",
    "node-sass-chokidar": "1.1.0",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.11",
    "react-redux": "5.0.7",
    "react-test-renderer": "16.2.0",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.2.11"
  }
}
